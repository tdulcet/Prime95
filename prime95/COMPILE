# Standard options
masm=masm /Mx /P /B63 

# Create 32-bit executables

factor64.obj: factor64.asm
    $(masm) factor64.asm factor64.obj NUL NUL

mult.obj: mult.asm mult.mac
    $(masm) mult.asm mult.obj NUL NUL

mult1.obj: mult1.asm mult.mac normal.mac
    $(masm) mult1.asm mult1.obj NUL NUL

mult1p.obj: mult1.asm mult.mac normal.mac
    $(masm) /DPPRO mult1.asm mult1p.obj NUL NUL

mult2.obj: mult2.asm mult.mac normal.mac
    $(masm) mult2.asm mult2.obj NUL NUL

